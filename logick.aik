ad $.d, $.s, $.t := 0:4 .d:4 .s:4 .t:4
an $.d, $.s, $.t := 1:4 .d:4 .s:4 .t:4
or $.d, $.s, $.t := 2:4 .d:4 .s:4 .t:4
